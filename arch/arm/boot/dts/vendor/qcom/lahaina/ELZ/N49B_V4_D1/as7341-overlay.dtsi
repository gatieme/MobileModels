/* Definitions for tcs3707 sensor from ams AG
 *
 * Compile:
 * dtc -@ -I dts -O dtb -o tcs3707-overlay.dtbo tcs3707-overlay.dts
 */


&qupv3_se10_i2c {
    status = "ok";
    ams_as7341@39 {
      compatible = "ams,ams_as7341";
      reg = <0x39>;
      colorsensor-vdd-supply = <&L8C>;
      flicker_support = <1>;
      algo_support = <1>;
      status = "okay";
    };
};

