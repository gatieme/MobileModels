#include "../huawei_charger/mt6853_ang_huawei_charger.dtsi"

&i2c7 {
	rt9426_battery@55{
		status = "disabled";
	};
	bq27z561_fg@55 {
		reg = <0x55>;
		compatible = "huawei,bq27z561";
		status = "ok";
	};
	bq25970@65 {
		status = "disabled";
	};
	bq25970@66 {
		i2c_recovery_addr = <0x65>;
		status = "ok";
	};
};

&i2c0 {
	bq25970@65 {
		status = "disabled";
	};
	bq25970@66 {
		i2c_recovery_addr = <0x65>;
		status = "ok";
	};
};

&multi_btb_temp {
	sensor-names =
		"mtk_batt_temp", "comp_para",
		"bq_battery", "comp_para1";
};
