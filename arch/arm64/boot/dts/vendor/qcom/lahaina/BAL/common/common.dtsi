/*
 * Each set of pins contains the pin number and pins voltage
 * values, and must end with the names _num and _val.
 */
&gpio_check {
	/* 432 : GPIO124, tlmm BASE = 308 */
	/* 433 : GPIO125, tlmm BASE = 308 */
	/* 434 : GPIO126, tlmm BASE = 308 */
	/* 436 : GPIO128, tlmm BASE = 308 */
	/* 488 : GPIO180, tlmm BASE = 488 */
	buckle_num = "432 433 434 436 488";
	buckle_val = "0 0 0 0 0";
};
&sensorhub_ext_config {
	compatible = "huawei,sensorhub";
	acc_sensor_id = "1 65558";// acc:1;  acc1:65558:65558=0x10016
};
&drop_sensor_enable {
    compatible = "huawei,sensorhub_drop";
    enable_drop = "1";
};
