COPTS +=-D_PRE_PLAT_FEATURE_CUSTOMIZE
#TAS
COPTS +=-D_PRE_WLAN_FEATURE_TAS_ANT_SWITCH

COPTS +=-D_PRE_WLAN_FEATURE_DFR

