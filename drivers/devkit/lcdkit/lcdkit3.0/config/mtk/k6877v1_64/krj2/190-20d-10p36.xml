<hwlcd>
	<PanelEntry>
		<!--Ldi config start-->
		<!--HBP setting-->
		<HBackPorch>54</HBackPorch>
		<!--HFP setting-->
		<HFrontPorch>946</HFrontPorch>
		<!--HS setting-->
		<HPulseWidth>20</HPulseWidth>
		<!--VBP setting-->
		<VBackPorch>86</VBackPorch>
		<!--VFP setting-->
		<VFrontPorch>26</VFrontPorch>
		<!--VS setting-->
		<VPulseWidth>4</VPulseWidth>

		<!--VReFresh setting-->
		<VReFresh>60</VReFresh>
		<!--Fps fuction-->
		<PanelFpsUpdtSupport>1</PanelFpsUpdtSupport>
		<!--DSC Params-->
		<DSCEnable>0</DSCEnable>

		<!-- 0-60hz, 1-H60hz, 2-90hz, 3-120hz, first is default -->
		<FpsCheckAfterPowerOnDelay>1</FpsCheckAfterPowerOnDelay>
		<PanelSupportFpsList>"1, 2"</PanelSupportFpsList>
		<!-- HFP-HBP-HS-VFP-VBP-VS-VRE -->
		<FpsToHigh60DsiTimming>"946, 54, 20, 26, 86, 4, 60, 982000, 0"</FpsToHigh60DsiTimming>
		<!-- HFP-HBP-HS-VFP-VBP-VS-VRE -->
		<FpsTo90DsiTimming>"210, 54, 20, 26, 86, 4, 90, 982000, 0"</FpsTo90DsiTimming>
		<!--panel switch to high 60hz code-->
		<PanelFpsHigh60Command>"0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x2A
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0x23, 0x10
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x26
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0x01, 0x70
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x10"</PanelFpsHigh60Command>
		<PanelFpsHigh60CmdState>1</PanelFpsHigh60CmdState>
		<!--panel switch to 120hz code-->
		<FpsTo90Command>"0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x2A
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0x23, 0x11
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x26
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0x01, 0x30
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x10"</FpsTo90Command>
		<FpsTo90CmdState>1</FpsTo90CmdState>
		<!--Mipi hopping support-->
		<MipiHoppingSupport>0</MipiHoppingSupport>
		<!-- HFP-HBP-HS-VFP-VBP-VS-RATE-LOWER -->
		<FpsToHigh60MipiHopping>"236, 40, 20, 1270, 10, 10, 1066, 1270"</FpsToHigh60MipiHopping>
		<!-- HFP-HBP-HS-VFP-VBP-VS-RATE-LOWER -->
		<FpsTo90MipiHopping>"236, 40, 20, 40, 10, 10, 1066, 40"</FpsTo90MipiHopping>
		<!--Tp Project Id-->
		<PanelDefaultTpProjectId>"T010AG130"</PanelDefaultTpProjectId>
		<!--Mipi Phy Mode 0:Dphy 1:Cphy-->
		<MipiPhyMode>1</MipiPhyMode>
		<!-- 0:cmd,1:plus-vdo,2:event-mode,3:burst-vdo -->
		<LdiDsiMode>3</LdiDsiMode>
		<!--Mipi lane number:c-phy:1-1lane, 2-2lane, 3-3lane d-phy:1-1lane, 2-2lane, 3-3lane, 4-4lane-->
		<MipiLaneNums>3</MipiLaneNums>
		<!--Panel information config start-->
		<!--Pixel clock-->
		<PanelPxlClk>491</PanelPxlClk>
		<!--Pixel clock division ratio-->
		<PanelPxlClkDiv>0</PanelPxlClkDiv>
		<!--mipi data rate pixel clock * 2-->
		<PanelDataRate>982</PanelDataRate>
		<PanelDataRateKhz>982000</PanelDataRateKhz>
		<!--Mipi non continue enable:0-continue mode, 1-non continue mode-->
		<MipiNonContinueEnable>1</MipiNonContinueEnable>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<!--Esd enable-->
		<PanelEsdSupport>1</PanelEsdSupport>
                <EsdGpioDetectSupport>1</EsdGpioDetectSupport>
                <EsdGpioDetectNum>16</EsdGpioDetectNum>
                <EsdGpioNormalValue>0</EsdGpioNormalValue>
                <PanelSkipEsdCheckCnt>1</PanelSkipEsdCheckCnt>
                <PanelSkipPowerOnEsdCheck>1</PanelSkipPowerOnEsdCheck>
                <PanelAgainEsdCheckDelay>150</PanelAgainEsdCheckDelay>
		<!-- Esd recovery backlight -->
		<PanelEsdRecoveryBlSupport>1</PanelEsdRecoveryBlSupport>
		<!--bl max type:0-not support, 1-ddic ytpe-->
		<PanelGetBlMaxNitType>0</PanelGetBlMaxNitType>
		<PanelThpProximitySupport>0</PanelThpProximitySupport>
		<ThpProximityResetDelayMin>72</ThpProximityResetDelayMin>
		<!--DoesLcdPowerOnTp-->
		<DoesLcdPowerOnTp>1</DoesLcdPowerOnTp>
		<!--bl max nit-->
		<PanelBlMaxnit>450</PanelBlMaxnit>
		<!--bl min brightness setting-->
		<PanelBlmin>33</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>4095</PanelBlmax>
		<!--lk bl max brightness setting-->
		<PanelBlBoot>234</PanelBlBoot>
		<!--bl ctrl type setting-->
		<PanelBlIcCtrlType>3</PanelBlIcCtrlType>
		<!--bias ctrl type setting-->
		<PanelBiasIcCtrlType>3</PanelBiasIcCtrlType>
		<!--gpio offset number setting-->
		<PanelGpioOffset>286</PanelGpioOffset>
		<!--Running test config start-->
		<!--Dot/column inversion test support-->
		<PanelInversionSupport>0</PanelInversionSupport>
		<!--Check panel power status test support-->
		<PanelCheckRegSupport>0</PanelCheckRegSupport>
		<!--Project id support-->
		<PanelProjectIdSupport>1</PanelProjectIdSupport>
		<!--PanelOnAlwaysNeedReset-->
		<PanelOnAlwaysNeedReset>1</PanelOnAlwaysNeedReset>
		<!--OemInfo support-->
		<OemInfoSupport>1</OemInfoSupport>
		<!--OemBarcode2DSupport support-->
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<!--OemBarcode2DNumOffset-->
		<OemBarcode2DNumOffset>1</OemBarcode2DNumOffset>
		<!--sn code support-->
		<PanelSnCodeSupport>1</PanelSnCodeSupport>
		<!--Panel quickly sleep out support-->
		<PanelQuicklySleepOutSupport>0</PanelQuicklySleepOutSupport>
		<PanelQuicklySleepOutInterval>72</PanelQuicklySleepOutInterval>
		<!--BTB check Support-->
		<LcdBtbSupport>0</LcdBtbSupport>
		<LcdBtbCheckType>0</LcdBtbCheckType>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early event, 8-TS later event, 9-vdd 10-AOD, 11-BIAS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 3
				11, 1, 2
				3, 1, 5
				4, 1, 10
				5, 2, 1
				5, 1, 5
				5, 2, 1
				5, 1, 11
				7, 1, 2
				6, 1, 0
				8, 1, 0"</PowerOnStage>
		<LPOnStage>"0, 0, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"7, 0, 0
			     8, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"6, 0, 0
				5, 0, 1
				11, 0, 5
				4, 0, 2
				3, 0, 5
				2, 0, 0"</PowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>1</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<LcdReset>"2, 40, 0"</LcdReset>
		<LcdIovcc>"2, 31, 0"</LcdIovcc>
		<LcdVsp>"2, 108, 6000000"</LcdVsp>
		<LcdVsn>"2, 109, 6000000"</LcdVsn>
		<PowerDownVsp>"2, 108, 4700000"</PowerDownVsp>
		<PowerDownVsn>"2, 109, 4700000"</PowerDownVsn>
		<!--event send-->
		<!--Mipi clk post adjust-->
		<MipiClkPostAdjust>0</MipiClkPostAdjust>
		<!--bl max nit-->
		<PanelGetBlMaxNitOffset>420</PanelGetBlMaxNitOffset>
		<PanelBlMaxNitCommand>"0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x01, 0xdb"</PanelBlMaxNitCommand>
		<PanelBlMaxNitCommandState>0</PanelBlMaxNitCommandState>
		<!--hs setting-->
		<PanelDsiHSTrail>12</PanelDsiHSTrail>
		<PanelDsiHSZero>35</PanelDsiHSZero>
		<PanelDsiHSPrpr>0xa</PanelDsiHSPrpr>
	</PanelEntry>
</hwlcd>
