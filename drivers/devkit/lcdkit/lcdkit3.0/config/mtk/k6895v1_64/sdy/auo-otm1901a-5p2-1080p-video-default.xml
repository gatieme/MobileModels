		<LcdIovcc>"2, 212, 1800000"</LcdIovcc>
		<LcdVci>"1, 68, 3000000"</LcdVci>
		<VciRegulatorName>"VFP"</VciRegulatorName>
		<LcdVdd>"2, 213, 1200000"</LcdVdd>
		<LcdReset>"2, 41, 0"</LcdReset>
		<LcdVsp>"0, 0, 0"</LcdVsp>
		<LcdVsn>"0, 0, 0"</LcdVsn>

