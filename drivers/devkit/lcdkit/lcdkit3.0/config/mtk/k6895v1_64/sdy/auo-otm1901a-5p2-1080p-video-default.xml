		<HSOnStage>"0, 0, 0"</HSOnStage>
		<LkPowerOnEarly>1</LkPowerOnEarly>
		<LkPowerOnEarlySeq>"16, 1, 1
                    2, 1, 2
                    9, 1, 3
                    1, 1, 11"</LkPowerOnEarlySeq>

		<!--Power off timming-->
		<HSOffStage>"0, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
