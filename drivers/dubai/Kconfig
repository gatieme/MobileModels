menu "Device Usage Big-data Analytics Initiative Support"

config POWER_DUBAI
	bool "Device Usage Big-data Analytics Initiative Support"
	default n
	select HUAWEI_DUBAI
	help
	  This driver provides DUBAI feature

config POWER_DUBAI_HISYSEVENT
	bool "Use hisysevent to send DUBAI kernel message"
	default n
	depends on POWER_DUBAI && HISYSEVENT
	help
	  This driver provides DUBAI hisysevent feature

config HUAWEI_DUBAI
	bool "Device Usage Big-data Analytics Initiative Support"
	default n
	help
	  This driver provides DUBAI feature

config POWER_DUBAI_TASK_CPU_POWER
	bool "DUBAI Task CPU Power"
	default n
	depends on POWER_DUBAI && (CPU_FREQ_POWER_STAT || ARCH_MSM)
	help
	  Get task cpu power

config POWER_DUBAI_RGB_STATS
	bool "DUBAI Rgb Stats"
	default n
	depends on POWER_DUBAI && FB
	help
	  Get Rgb Stats

config POWER_DUBAI_LTPO_REFRESH_RATE
	bool "DUBAI let LTPO refreshrate statistic only compile in new platform"
	default n
	help
	  This driver limits the compile version

config HUAWEI_DUBAI_TASK_CPU_POWER
	bool "Huawei DUBAI Task CPU Power"
	default n
	depends on HUAWEI_DUBAI && CPU_FREQ_POWER_STAT
	help
	  Get task cpu power

config HUAWEI_DUBAI_RGB_STATS
	bool "Huawei DUBAI Rgb Stats"
	default n
	depends on HUAWEI_DUBAI && FB
	help
	  Get Rgb Stats

endmenu
