config HUAWEI_DUBAI
    tristate "Dubai State"
    help
        Dubai report state, please choose it.

config HUAWEI_DUBAI_BATTERY_STATS
	bool "Dubai battery stats (capacity_rm)"
	default n
	depends on HUAWEI_DUBAI
	help
		This config is mainly for getting capacity_rm value of power_supply.
		Please make sure there is POWER_SUPPLY_PROP_CAPACITY_RM.

config HUAWEI_DUBAI_GPU_STATS
	bool "Dubai gpu frequency stats"
	default n
	depends on HUAWEI_DUBAI

config HUAWEI_DUBAI_DDR_STATS
	bool "Dubai ddr stats"
	default n
	depends on HUAWEI_DUBAI
