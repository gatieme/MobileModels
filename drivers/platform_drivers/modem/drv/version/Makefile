
CURRENT_VERSION_BUILD_DATE = $(shell date +'%b %d %Y')
CURRENT_VERSION_BUILD_TIME = $(shell date +'%T')
KBUILD_CFLAGS += -DVERSION_BUILD_DATE="\"$(CURRENT_VERSION_BUILD_DATE)\""
KBUILD_CFLAGS += -DVERSION_BUILD_TIME="\"$(CURRENT_VERSION_BUILD_TIME)\""

obj-y					:= version_driver.o
GCOV_PROFILE := y

