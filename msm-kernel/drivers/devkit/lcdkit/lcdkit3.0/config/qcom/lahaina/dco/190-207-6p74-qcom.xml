<hwlcd>
	<PanelEntry>
		<!--Panel type:dsi_video_mode-video mode, dsi_cmd_mode-command mode-->
		<PanelTypeMode>"dsi_cmd_mode"</PanelTypeMode>
		<PanelModeSwitch>0</PanelModeSwitch>
		<!--active area size-->
		<PanelPhyWidth>72</PanelPhyWidth>
		<PanelPhyHeight>155</PanelPhyHeight>

		<!--Dsi ctrl number:1-one dsi ctrl, 2-two dsi ctrl-->
		<DsiCtrlNum>0</DsiCtrlNum>
		<!--Dsi phy number:1-one dsi phy, 2-two dsi phy-->
		<DsiPhyNum>0</DsiPhyNum>

		<!-- Lane Config -->
		<PanelDSILaneMap>"lane_map_0123"</PanelDSILaneMap>
		<PanelLane0State>1</PanelLane0State>
		<PanelLane1State>1</PanelLane1State>
		<PanelLane2State>1</PanelLane2State>
		<PanelLane3State>1</PanelLane3State>

		<!-- MIPI Host Config -->
		<DsiSelectClocks>"mux_byte_clk0", "mux_pixel_clk0"</DsiSelectClocks>

		<!-- Display dsi timming config -->
		<DisplayDsiTimmingBegin>1</DisplayDsiTimmingBegin>
		<DisplayDsiTimming0Begin>1</DisplayDsiTimming0Begin>
		<DisplayDsiTimming0End>1</DisplayDsiTimming0End>
		<DisplayDsiTimming1Begin>1</DisplayDsiTimming1Begin>
		<DisplayDsiTimming1End>1</DisplayDsiTimming1End>
		<DisplayDsiTimming2Begin>1</DisplayDsiTimming2Begin>
		<DisplayDsiTimming2End>1</DisplayDsiTimming2End>
		<DisplayDsiTimmingEnd>1</DisplayDsiTimmingEnd>

		<!-- Display command panel config -->
		<PanelTEPinSelect>1</PanelTEPinSelect>
		<PanelWRMEMStart>0x2c</PanelWRMEMStart>
		<PanelWRMEMContinue>0x3c</PanelWRMEMContinue>
		<PanelTEDCSCmd>1</PanelTEDCSCmd>
		<PanelTECheckEnable>1</PanelTECheckEnable>
		<PanelTEUsingTEPin>1</PanelTEUsingTEPin>
		<PanelGpioAddress>0x0F100000 0x1000</PanelGpioAddress>
		<PanelGpioPins>12 24 82</PanelGpioPins>
		<PanelGpioNames>"reg-gpio", "reset-gpio", "platform-te-gpio"</PanelGpioNames>

		<!-- Display dsi timming0 config -->
		<PanelXres0>1212</PanelXres0>
		<PanelYres0>2616</PanelYres0>
		<HFrontPorch0>32</HFrontPorch0>
		<HBackPorch0>32</HBackPorch0>
		<HPulseWidth0>32</HPulseWidth0>
		<VFrontPorch0>16</VFrontPorch0>
		<VBackPorch0>12</VBackPorch0>
		<VPulseWidth0>4</VPulseWidth0>
		<HSyncSkew0>0</HSyncSkew0>
		<PanelFrameRate0>60</PanelFrameRate0>
		<PanelPhyTimings0>"00 12 04 04 12 1E 04
			05 05 03 02 04 0F 09"</PanelPhyTimings0>
		<DisplayTopology0>2 2 1</DisplayTopology0>
		<DefaultTopologyIndex0>0</DefaultTopologyIndex0>
		<PanelCompressionMode0>"dsc"</PanelCompressionMode0>
		<PanelDscSliceHeight0>24</PanelDscSliceHeight0>
		<PanelDscSliceWidth0>606</PanelDscSliceWidth0>
		<PanelDscSlicePerPkt0>2</PanelDscSlicePerPkt0>
		<PanelDscBitPerComponent0>8</PanelDscBitPerComponent0>
		<PanelDscBitPerPixel0>8</PanelDscBitPerPixel0>
		<PanelDscBlockPredictionEnable0>1</PanelDscBlockPredictionEnable0>

		<!-- Display dsi timming1 config -->
		<PanelXres1>1212</PanelXres1>
		<PanelYres1>2616</PanelYres1>
		<HFrontPorch1>32</HFrontPorch1>
		<HBackPorch1>32</HBackPorch1>
		<HPulseWidth1>32</HPulseWidth1>
		<VFrontPorch1>16</VFrontPorch1>
		<VBackPorch1>12</VBackPorch1>
		<VPulseWidth1>4</VPulseWidth1>
		<HSyncSkew1>0</HSyncSkew1>
		<PanelFrameRate1>90</PanelFrameRate1>
		<PanelPhyTimings1>"00 19 06 06 15 20 07
			06 06 07 02 04 16 0B"</PanelPhyTimings1>
		<DisplayTopology1>2 2 1</DisplayTopology1>
		<DefaultTopologyIndex1>0</DefaultTopologyIndex1>
		<PanelCompressionMode1>"dsc"</PanelCompressionMode1>
		<PanelDscSliceHeight1>24</PanelDscSliceHeight1>
		<PanelDscSliceWidth1>606</PanelDscSliceWidth1>
		<PanelDscSlicePerPkt1>2</PanelDscSlicePerPkt1>
		<PanelDscBitPerComponent1>8</PanelDscBitPerComponent1>
		<PanelDscBitPerPixel1>8</PanelDscBitPerPixel1>
		<PanelDscBlockPredictionEnable1>1</PanelDscBlockPredictionEnable1>

		<!-- Display dsi timming2 config -->
		<PanelXres2>1212</PanelXres2>
		<PanelYres2>2616</PanelYres2>
		<HFrontPorch2>32</HFrontPorch2>
		<HBackPorch2>32</HBackPorch2>
		<HPulseWidth2>32</HPulseWidth2>
		<VFrontPorch2>16</VFrontPorch2>
		<VBackPorch2>12</VBackPorch2>
		<VPulseWidth2>4</VPulseWidth2>
		<HSyncSkew2>0</HSyncSkew2>
		<PanelFrameRate2>120</PanelFrameRate2>
		<PanelPhyTimings2>"00 20 08 08 18 17 08
			08 08 08 02 04 1B 0D"</PanelPhyTimings2>
		<DisplayTopology2>2 2 1</DisplayTopology2>
		<DefaultTopologyIndex2>0</DefaultTopologyIndex2>
		<PanelCompressionMode2>"dsc"</PanelCompressionMode2>
		<PanelDscSliceHeight2>24</PanelDscSliceHeight2>
		<PanelDscSliceWidth2>606</PanelDscSliceWidth2>
		<PanelDscSlicePerPkt2>2</PanelDscSlicePerPkt2>
		<PanelDscBitPerComponent2>8</PanelDscBitPerComponent2>
		<PanelDscBitPerPixel2>8</PanelDscBitPerPixel2>
		<PanelDscBlockPredictionEnable2>1</PanelDscBlockPredictionEnable2>

		<!-- Backlight ctrl config -->
		<BlPmicControlType>"bl_ctrl_dcs"</BlPmicControlType>
		<MdssDsiBlMinLevel>1</MdssDsiBlMinLevel>
		<MdssDsiBlMaxLevel>4095</MdssDsiBlMaxLevel>
		<DsiPanelCphyMode>0</DsiPanelCphyMode>

		<!--Project id support-->
		<PanelProjectIdSupport>0</PanelProjectIdSupport>
		<!--Tp Project Id-->
		<PanelDefaultTpProjectId>"G240A7130"</PanelDefaultTpProjectId>

		<!--OemInfo support-->
		<OemInfoSupport>1</OemInfoSupport>
		<!--OemBarcode2DSupport support-->
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<!--OemBarcode2DNumOffset-->
		<OemBarcode2DNumOffset>0</OemBarcode2DNumOffset>
		<!--sn code support-->
		<PanelSnCodeSupport>1</PanelSnCodeSupport>

		<!--Lcdkit fps support -->
		<PanelFpsUpdtSupport>1</PanelFpsUpdtSupport>
		<!--Lcdkit default fps -->
		<PanelDefaultFps>60</PanelDefaultFps>
		<!--Lcdkit fps support list -->
		<PanelSupportFpsList>"60, 90, 120"</PanelSupportFpsList>
		<!--qcom fps config -->
		<DsiPanelDynamicModeSwitchEnabled>1</DsiPanelDynamicModeSwitchEnabled>
		<DsiPanelDynamicModeSwitchType>"dynamic-resolution-switch-immediate"</DsiPanelDynamicModeSwitchType>
		<PanelFPS60CMD>"vndservice call display.qservice 25 i32 0 i32 0"</PanelFPS60CMD>
		<PanelFPS90CMD>"vndservice call display.qservice 25 i32 2 i32 0"</PanelFPS90CMD>
		<PanelFPS120CMD>"vndservice call display.qservice 25 i32 1 i32 0"</PanelFPS120CMD>

		<!--bl min brightness setting-->
		<PanelBlmin>4</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>4095</PanelBlmax>
		<!--Running test config start-->
		<!--Dot/column inversion test support-->
		<PanelInversionSupport>0</PanelInversionSupport>
		<!--Check panel power status test support-->
		<FactoryCheckRegSupport>0</FactoryCheckRegSupport>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<!--gpio offset number setting-->
		<PanelGpioOffset>308</PanelGpioOffset>
		<!--Thp Proximity support-->
		<PanelThpProximitySupport>0</PanelThpProximitySupport>
		<ThpProximityResetDelayMin>65</ThpProximityResetDelayMin>
		<!--PanelOnAlwaysNeedReset-->
		<PanelOnAlwaysNeedReset>0</PanelOnAlwaysNeedReset>

		<!-- If LP11_INIT is set, panel will be powered up during prepare() , -->
		<!-- If not set, will be powered up during pre_prepare()-->
		<DsiLp11Init>0</DsiLp11Init>
		<!-- if ThreeStagePowerOn is set,poweron sequence: PowerOnStage LPOnStage HSOnStage-->
		<!-- if not set,poweron sequence: PowerOnStage LPOnStage-->
		<ThreeStagePowerOn>1</ThreeStagePowerOn>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early event, 8-TS later event, 9-vdd 10-AOD, 11-BIAS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 1
				3, 1, 1
				1, 1, 10"</PowerOnStage>
		<LPOnStage>"5, 1, 10
				7, 1, 0
				6, 1, 0
				8, 1, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"7, 0, 0
				6, 0, 0
				8, 0, 0
				5, 0, 1
				1, 0, 1
				3, 0, 1
				2, 0, 1"</PowerOffStage>
		<OnlyPowerOffStage>"0, 0, 0"</OnlyPowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>0</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		when mode is regulate to set min voltage, max voltage, enable load, disable load
		mode, number, amplitude, min voltage, max voltage, enable load, disable load
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		min voltage:1850000-1.85v
		max voltage:5500000-5.5v
		enable load:62000
		disable load:80
		-->
		<LcdReset>"2, 24, 0"</LcdReset>
		<LcdIovcc>"1, 12, 1800000, 1800000, 1800000, 60700, 80"</LcdIovcc>
		<LcdVci>"1, 13, 3000000, 3000000, 3000000, 10000, 80"</LcdVci>
		<LcdVsp>"1, 1, 1200000, 1200000, 1200000, 60700, 80"</LcdVsp>
		<IovccName>"vddio"</IovccName>
		<VciName>"vdd"</VciName>
		<VspName>"ddic"</VspName>
		<!--event send-->
		<!--Ap aod support-->
		<ApAodSupport>1</ApAodSupport>
		<!--oneside driver mode detect test-->
		<OnesideModeSupport>0</OnesideModeSupport>
		<!--color aod detect-->
		<ColorAodDetectSupport>1</ColorAodDetectSupport>
		<!--pcd errflag check support-->
		<GpioPcdErrflag>0</GpioPcdErrflag>
		<PanelPcdErrflagCheckSupport>0</PanelPcdErrflagCheckSupport>
		<PcdCmdsSupport>0</PcdCmdsSupport>
		<!--Pcd reg-value compare mode:
		0: == NG, != ok
		1: >= NG, < ok
		-->
		<PcdCmdsSupportCompareMode>1</PcdCmdsSupportCompareMode>
		<!--ddic low voltage detect test-->
		<DdicLVDetectSupport>0</DdicLVDetectSupport>
		<!--horizontal/vertical line test-->
		<VtcLineSupport>0</VtcLineSupport>
		<HorLineSupport>0</HorLineSupport>
		<HorLineDuration>600</HorLineDuration>
		<!--elvdd detect-->
		<PanelElvddDetectSupport>0</PanelElvddDetectSupport>
		<!--elvdd detect type-->
		<ElvddDetectType>2</ElvddDetectType>
		<ElvddDetectGpio>104</ElvddDetectGpio>
		<ElvddDetectValue>1</ElvddDetectValue>
		<ElvddDelay>50</ElvddDelay>
		<!--checksum support-->
		<PanelLogoChecksumSupport>0</PanelLogoChecksumSupport>
		<PanelChecksumSupport>0</PanelChecksumSupport>
		<ChecksumStressTestSupport>0</ChecksumStressTestSupport>
		<ChecksumStressTestVdd>1280000</ChecksumStressTestVdd>
		<ChecksumStressTestMipi>550</ChecksumStressTestMipi>
		<!--Hbm support-->
		<PanelHbmCtrlSupport>1</PanelHbmCtrlSupport>
		<LocalHBMSupport>0</LocalHBMSupport>
		<!--ddic alpha support-->
		<AlphaSupport>1</AlphaSupport>
		<!--force delta bl update support-->
		<ForceDeltaBlUpdateSupport>1</ForceDeltaBlUpdateSupport>
		<!-- for ic spr and ecc params, will be set after kernel panel on, cannot be set in panelon command due to too many commands -->
		<PanelScreenOnEffectSupport>1</PanelScreenOnEffectSupport>
		<!--Is Alpha with Enable flag-->
		<AlphaWithEnableFlag>1</AlphaWithEnableFlag>
		<NodeGrayscale>"159, 175, 191, 207, 223, 255, 255"</NodeGrayscale>
		<!--Need update dynamic frame to 90hz when lcd on, Jade - Local hbm-->
		<PanelFpsUpdateTo90>1</PanelFpsUpdateTo90>
	</PanelEntry>
</hwlcd>
