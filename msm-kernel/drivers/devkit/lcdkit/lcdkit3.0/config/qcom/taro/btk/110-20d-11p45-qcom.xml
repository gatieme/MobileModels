		<LcdVci>"2, 16, 1"</LcdVci>
		<LcdReset>"2, 42, 0"</LcdReset>
		<TpReset>"2, 17, 0"</TpReset>
		<LcdVsp>"2, 13, 6000000"</LcdVsp>
		<LcdVsn>"2, 14, 6000000"</LcdVsn>
		<PowerDownVsp>"2, 13, 4700000"</PowerDownVsp>
		<PowerDownVsn>"2, 14, 4700000"</PowerDownVsn>
