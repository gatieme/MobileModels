config MM_LB
	bool "last buffer"
	help
	  Enable last buffer.

	  If unsure, say N here.

config MM_LB_DEBUG
	bool "Lb debug"
	depends on MM_LB
	help
	  Enable last buffer debug.

	  If unsure, say N here.

config MM_LB_ENG_DEBUG
	bool "Lb eng debug"
	depends on MM_LB
	help
	  Enable last buffer eng debug.

	  If unsure, say N here.

config MM_LB_TST
	bool "Lb ip test"
	depends on MM_LB
	help
	  Enable last buffer ip test
	  and need disable lb debug

	  if unsure, say N here

