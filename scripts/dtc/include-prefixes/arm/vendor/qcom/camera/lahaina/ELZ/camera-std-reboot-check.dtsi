	};

	cam_sensor_active_front_dvdd_en: cam_sensor_active_front_dvdd_en {
		/* dvdd enable */
		mux {
			pins = "gpio37";
			function = "gpio";
		};

		config {
			pins = "gpio37";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_suspend_front_dvdd_en: cam_sensor_suspend_front_dvdd_en {
		/* dvdd disable */
		mux {
			pins = "gpio37";
			function = "gpio";
		};

		config {
			pins = "gpio37";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
