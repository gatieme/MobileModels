	laser_xshut_high: laser_xshut_high {
		/* laser xshut enable */
		mux {
			pins = "gpio75";
			function = "gpio";
		};

		config {
			pins = "gpio75";
			bias-disable; /* No PULL */
			output-high;
			drive-strength = <2>; /* 2 MA */
		};
	};

	laser_xshut_low: laser_xshut_low {
		/* laser xshut disable */
		mux {
			pins = "gpio75";
			function = "gpio";
		};

		config {
			pins = "gpio75";
