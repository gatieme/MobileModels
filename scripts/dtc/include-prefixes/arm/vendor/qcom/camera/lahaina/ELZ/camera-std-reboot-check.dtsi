		};
	};

	laser_intr: laser_intr {
		/* laser intr */
		mux {
			pins = "gpio74";
			function = "gpio";
		};

		config {
			pins = "gpio74";
			input-enable;
			bias-pull-up;
			drive-strength = <2>; /* 2 MA */
		};
	};
};

&qupv3_se17_i2c {
	status = "ok";
	honor,stmvl53l3@29 {
		compatible = "honor,stmvl53l3";
		reg = <0x29>;
		gpios = <&tlmm 75 0>,<&tlmm 74 0>;
		honor,gpio-ctrl-types = "xsdn", "intr";
		pinctrl-names = "laser_xshut_high", "laser_xshut_low", "laser_intr";
		pinctrl-0 = <&laser_xshut_high>;
		pinctrl-1 = <&laser_xshut_low>;
		pinctrl-2 = <&laser_intr>;
		laser_state = <1>;
		status = "ok";
	};
};

