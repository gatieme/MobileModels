#include <dt-bindings/clock/qcom,camcc-lahaina.h>
#include "../isolate-qrd.dtsi"

&product_name_camera {
	product-name = "ELZ";
	reboot-check = "1";
};

&pm8350c_flash {
	open-short-mask = /bits/ 8 <0xc3>;
};

&pm8008i_regulator {
	compatible = "qcom,pm8008-regulator";
	#address-cells = <1>;
	#size-cells = <0>;

	pm8008_en-supply = <&PM8008I_EN>;
	vdd_l1_l2-supply = <&S12B>;
	vdd_l3_l4-supply = <&BOB>;
	vdd_l5-supply = <&BOB>;
	vdd_l6-supply = <&S1C>;
	vdd_l7-supply = <&BOB>;
};

&pm8008i_l1 {
	regulator-min-microvolt = <1000000>;
	regulator-max-microvolt = <1200000>;
	qcom,min-dropout-voltage = <224000>;
};

&pm8008i_l2 {
	regulator-min-microvolt = <1000000>;
	regulator-max-microvolt = <1250000>;
	qcom,min-dropout-voltage = <150000>;
};

&pm8008i_l3 {
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <3400000>;
	qcom,min-dropout-voltage = <200000>;
};

&pm8008i_l4 {
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <3400000>;
	qcom,min-dropout-voltage = <0>;
};

&pm8008i_l5 {
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <3400000>;
	qcom,min-dropout-voltage = <70000>;
};

&pm8008i_l6 {
	regulator-min-microvolt = <1728000>;
	regulator-max-microvolt = <1800000>;
	qcom,min-dropout-voltage = <70000>;
};

&pm8008i_l7 {
	regulator-min-microvolt = <2800000>;
	regulator-max-microvolt = <3000000>;
	qcom,min-dropout-voltage = <70000>;
};

&soc {
	led_flash_rear: qcom,camera-flash@0 {
		cell-index = <4>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch2>;
		torch-light-num = <0>;
		torch-light-name = "torch-light0";
		flash_thermal;
		status = "ok";
	};

	led_flash_wide: qcom,camera-flash@1 {
		cell-index = <5>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch2>;
		status = "ok";
	};

	led_flash_mono: qcom,camera-flash@2 {
		cell-index = <6>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch2>;
		status = "ok";
	};

	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		status = "ok";
	};
};

&tlmm {
	cam_sensor_mclk0_active: cam_sensor_mclk0_active {
		/* MCLK0 */
		mux {
			pins = "gpio100";
			function = "cam_mclk";
		};

		config {
			pins = "gpio100";
			bias-disable; /* No PULL */
			drive-strength = <4>; /* 4 MA */
		};
	};

	cam_sensor_mclk0_suspend: cam_sensor_mclk0_suspend {
		/* MCLK0 */
		mux {
			pins = "gpio100";
			function = "cam_mclk";
		};

		config {
			pins = "gpio100";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <4>; /* 4 MA */
		};
	};

	cam_sensor_mclk1_active: cam_sensor_mclk1_active {
		/* MCLK1 */
		mux {
			pins = "gpio101";
			function = "cam_mclk";
		};

		config {
			pins = "gpio101";
			bias-disable; /* No PULL */
			drive-strength = <4>; /* 4 MA */
		};
	};

	cam_sensor_mclk1_suspend: cam_sensor_mclk1_suspend {
