&qupv3_se0_i2c {
	qcom,clk-freq-out = <400000>; /* if bq27z561_fg is used, i2c clock must be 100k(100000) */
	status = "ok";
	bq25970@65 {
		compatible = "bq25970"; /* BQ25970YFFR SC */
		pinctrl-names = "default";
		pinctrl-0 = <&bq25970_aux_irq_pins_default>;
		gpio_int = <&tlmm 79 0>;
		switching_frequency = <675>;
		sense_r_config = <20>;
		sense_r_actual = <10>;
		reg = <0x65>;
		ic_role = <1>;
		status = "disabled";
	};

	bq25970@66 {
		compatible = "bq25970"; /* VerB:RT9759WSC VerA:SC8551*/
		pinctrl-names = "default";
		pinctrl-0 = <&bq25970_aux_irq_pins_default>;
		gpio_int = <&tlmm 79 0>;
		switching_frequency = <675>;
		sense_r_config = <20>;
		sense_r_actual = <10>;
		reg = <0x66>;
		ic_role = <1>;
		status = "disabled";
	};
	rt9759@65 {
		compatible = "bq25970"; /* BQ25970YFFR SC */
		pinctrl-names = "default";
		pinctrl-0 = <&rt9759_aux_irq_pins_default>;
		gpio_int = <&tlmm 79 0>;
		switching_frequency = <675>;
		sense_r_config = <20>;
		sense_r_actual = <10>;
		reg = <0x65>;
		ic_role = <1>;
		i2c_recovery_addr = <0x66>;
		status = "ok";
	};
	rt9759@66 {
		compatible = "bq25970"; /* VerB:RT9759WSC VerA:SC8551*/
		pinctrl-names = "default";
		pinctrl-0 = <&rt9759_aux_irq_pins_default>;
		gpio_int = <&tlmm 79 0>;
		switching_frequency = <675>;
		sense_r_config = <20>;
		sense_r_actual = <10>;
		reg = <0x66>;
		ic_role = <1>;
		status = "disabled";
	};

	stwlc88@61 {
		compatible = "huawei, wireless_stwlc88";
		reg = <0x61>;
		pinctrl-names = "default";
		pinctrl-0 = <&stwlc88_irq_pins_default>;
		gpio_int = <&tlmm 7 0>;
		gpio_en = <&pm8350c_gpios 6 0>;
		gpio_sleep_en = <&tlmm 127 0x0>;
		gpio_pwr_good = <&tlmm 6 0x0>;
		gpio_en_valid_val = <0>;
		irq_no_suspend = <0>;
		rx_ss_good_lth = <110>;
		rx_ldo_cfg_5v = /bits/ 8 <0x13 0x13 0x0D 0x03 0x13 0x20 0x2C>;
		rx_ldo_cfg_9v = /bits/ 8 <0x13 0x13 0x0D 0x03 0x13 0x20 0x2C>;
		rx_ldo_cfg_12v = /bits/ 8 <0x13 0x13 0x0D 0x03 0x13 0x20 0x2C>;
		rx_ldo_cfg_sc = /bits/ 8 <0x9D 0x5D 0x0D 0x03 0x13 0x20 0x2C>;
		rx_fod_5v = /bits/ 8 <0x0A 0x0F 0x26 0x46 0x6E 0x00 0x00 0x00 0x00 0x00 0x00 0x18 0x17 0x18 0x16 0x13 0x11 0x8B>;
		rx_fod_9v = /bits/ 8 <0x31 0x42 0x54 0x78 0x82 0x00 0x00 0x00 0x00 0x00 0x00 0x32 0x32 0x32 0x1A 0x1A 0x0A 0x8C>;
		rx_fod_12v = /bits/ 8 <0x31 0x42 0x54 0x7C 0x91 0x00 0x00 0x00 0x00 0x00 0x00 0x13 0x0D 0x08 0xFC 0xE1 0xCE 0x8C>;
		rx_fod_15v = /bits/ 8 <0x72 0x7E 0xD4 0xDC 0xFA 0x00 0x00 0x00 0x00 0x00 0x00 0x59 0x70 0x64 0x28 0xCB 0x84 0x8B>;
		temp_para = <
			/* temp(min,max] ilim temp_back */
			0	100 	  2500 10
			100 999 	  1500 0
		>;
		time_para = <
			/* time  ilim */
			300   2000
			0	  2500
		>;
		product_isolation = <1>;
		status = "disabled";
	};
	wireless_cps4057@30 {
		compatible = "huawei, wireless_cps4057";
		reg = <0x30>;
		pinctrl-names = "default";
		irq_no_suspend = <0>;
		pinctrl-0 = <&stwlc88_irq_pins_default>;
		gpio_int = <&tlmm 7 0x0>;
		gpio_en = <&pm8350c_gpios 6 0>;
		gpio_sleep_en = <&tlmm 127 0x0>;
		gpio_pwr_good = <&tlmm 6 0x0>;
		rx_fod_5v = /bits/ 8 <0x00 0x19 0x32 0x4B 0x64 0x7D 0x96 0xAF 0x40 0x09 0x40 0x09 0x40 0x09 0x40 0x09 0x40 0x09 0x40 0x09 0x40 0x09 0x40 0x09>;
		rx_fod_9v = /bits/ 8 <0x00 0x19 0x32 0x4B 0x64 0x7D 0x96 0xAF 0x28 0x0A 0x28 0x0A 0x28 0x0A 0x28 0x0A 0x28 0x0A 0x28 0x0A 0x28 0x0A 0x28 0x0A>;
		rx_fod_15v = /bits/ 8 <0x00 0x19 0x32 0x4B 0x64 0x7D 0x96 0xAF 0x26 0x0F 0x26 0x0F 0x26 0x0F 0x26 0x0F 0x26 0x0F 0x26 0x0F 0x26 0x0F 0x26 0x0F>;
		tx_ploss_th0 = <2000>; /* 5v full bridge */
		tx_ploss_th1 = <2500>; /* 10v half bridge */
		tx_ploss_th2 = <1700>; /* 10v full bridge low voltage */
		tx_ploss_th3 = <2400>; /* 10v full bridge high voltage */
		tx_ploss_cnt = <3>;
		full_bridge_ith = <400>;
		rx_ldo_cfg_5v = /bits/ 8 <0x32 0x00 0x90 0x01 0x2C 0x01 0x64 0x00>;
		rx_ldo_cfg_9v = /bits/ 8 <0x32 0x00 0x20 0x03 0x2C 0x01 0x64 0x00>;
		rx_ldo_cfg_sc = /bits/ 8 <0x32 0x00 0xD0 0x09 0x2C 0x01 0x64 0x00>;
		time_para = <
			/* time  ilim */
			300   2000
			0     2500
		>;
		temp_para = <
			/* temp(min,max] ilim temp_back */
			0   100       2500 10
			100 999       1500 0
		>;
		support_multi_tx = <0>;
		#include "../../../huawei_charger/cps4057_ELZ_fodpara_pro.dtsi"
		product_isolation = <1>;
		status = "disabled";
	};

	pca9488@55 {
		compatible = "charge_pump_pca9488";
		reg = <0x55>;
		pinctrl-names = "default";
		pinctrl-0 = <&pca9488_irq_pins_default>;
		gpio_int = <&tlmm 85 0x0>;
		status = "disabled";
	};
	charge_pump_hl1512@51 {
		compatible = "charge_pump_hl1512";
		reg = <0x51>;
		gpio_int = <&tlmm 85 0x0>;
		status = "disabled";
	};
};

