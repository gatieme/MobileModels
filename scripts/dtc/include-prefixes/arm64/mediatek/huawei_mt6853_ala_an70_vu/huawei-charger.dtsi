#include "../huawei_charger/mt6853_ang_huawei_charger.dtsi"
&i2c7 {
	rt9426_battery@55{
		status = "disabled";
	};
	bq27z561_fg@55 {
		reg = <0x55>;
		compatible = "huawei,bq27z561";
		status = "disabled";
	};
	bq25970@65 {
		status = "disabled";
	};
	bq25970@66 {
		i2c_recovery_addr = <0x65>;
		status = "ok";
	};
};

&i2c0 {
	bq25970@65 {
		status = "disabled";
	};
	bq25970@66 {
		i2c_recovery_addr = <0x65>;
		status = "ok";
	};
};

&huawei_battery {
	use_third_coul = <0>;
};

&huawei_power_proxy  {
	status = "disabled";
};

&huawei_power_supply {
	status = "disabled";
};

&battery_core {
	status = "disabled";
};

&battery_model {
	status = "disabled";
};

&battery_ui_capacity {
	status = "disabled";
};

&bat_gm30 {
	status = "ok";
};

&multi_btb_temp {
	sensor-names =
		"mtk_batt_temp", "comp_para",
		"mtk_batt_temp", "comp_para";
};

&soc_decimal {
    ui_offset = <50>;
};
