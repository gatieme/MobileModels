#include "../huawei_charger/mt6853_hjc_huawei_charger.dtsi"

&i2c7 {
	bq25970@65 {
		i2c_recovery_addr = <0x66>;
		status = "ok";
	};

	rt9426_battery@55{
		status = "disabled";
	};
};

&i2c0 {
	bq25970@65 {
		i2c_recovery_addr = <0x66>;
		status = "ok";
	};
};

&huawei_battery {
	use_third_coul = <0>;
};

&huawei_power_proxy  {
	status = "disabled";
};
&huawei_power_supply {
	status = "disabled";
};
&battery_core {
	status = "disabled";
};
&battery_fault {
	status = "disabled";
};
&battery_model {
	status = "disabled";
};
&battery_ui_capacity {
	status = "disabled";
};

&bat_gm30 {
	status = "ok";
};

&multi_btb_temp {
	status = "ok";
	sensor-names =
		"mtk_batt_temp", "comp_para",
		"mtk_batt_temp", "comp_para";
};

&huawei_batt_info {
	shield_ct_sign = <1>;
};

&soc_decimal {
    ui_offset = <50>;
};
